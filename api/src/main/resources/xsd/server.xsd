            <xs:attribute type="xs:byte" name="transcoderVideo"/>
            <xs:attribute type="xs:string" name="transcoderVideoBitrates"/>
            <xs:attribute type="xs:string" name="transcoderVideoQualities"/>
            <xs:attribute type="xs:string" name="transcoderVideoResolutions"/>
            <xs:attribute type="xs:int" name="updatedAt"/>
