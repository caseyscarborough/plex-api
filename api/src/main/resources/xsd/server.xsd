            <xs:attribute type="xs:byte" name="itemClusters"/>
            <xs:attribute type="xs:byte" name="livetv"/>
            <xs:attribute type="xs:string" name="machineIdentifier"/>
            <xs:attribute type="xs:byte" name="mediaProviders"/>
            <xs:attribute type="xs:byte" name="multiuser"/>
            <xs:attribute type="xs:byte" name="myPlex"/>
